{
  "expo": {
    "name": "Vetra",
    "slug": "Vetra",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vetra-app-icon.png",
    "scheme": "vetrapos",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/vetra.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/vetra-app-icon.png",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to save reports to your photo library",
        "NSPhotoLibraryAddUsageDescription": "We need access to save reports to your photo library"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FF9F43"
      },
      "icon": "./assets/images/vetra-app-icon.png",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.MEDIA_LIBRARY"
      ],
      "package": "com.reymundabelgas.Vetra"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/vetra.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-barcode-scanner",
      "expo-media-library"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "81e851f9-ac8c-4b29-aa84-eaf54ed67ca7"
      }
    }
  }
}
